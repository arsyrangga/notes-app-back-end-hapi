{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style" : "off",
        "no-console": "off",
        "no-unused-vars" : "off",
        "import/no-extraneous-dependencies" : "off"
    }
}
